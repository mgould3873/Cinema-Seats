      </ImageView>
      <Button fx:id="backToMovieListButtion" layoutX="14.0" layoutY="561.0" mnemonicParsing="false" text="Back to movie/time selection" />
      <ImageView fitHeight="327.0" fitWidth="454.0" layoutX="173.0" layoutY="137.0" pickOnBounds="true" preserveRatio="true">
