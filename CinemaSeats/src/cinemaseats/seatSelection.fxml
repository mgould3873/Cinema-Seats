            <Image url="@../cope-seating-chart.jpg" />
         </image>
      </ImageView>
      <Button fx:id="reserveSeatsButton" layoutX="697.0" layoutY="561.0" mnemonicParsing="false" text="Reserve Seats" />
      <Text fill="WHITE" layoutX="226.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select the seats you wish to reserve:">
