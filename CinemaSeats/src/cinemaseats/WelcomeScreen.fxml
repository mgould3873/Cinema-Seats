<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" fx:id="welcomeScreenAnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cinemaseats.WelcomeScreenController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0">
         <image>
            <Image url="@../movietheater.png" />
         </image>
      </ImageView>
      <TextField editable="false" focusTraversable="false" layoutX="220.0" layoutY="14.0" prefHeight="58.0" prefWidth="360.0" text="Welcome to Cinema Seats">
         <font>
            <Font name="Bodoni MT" size="30.0" />
         </font>
      </TextField>
      <ChoiceBox fx:id="theaterChoiceBox" layoutX="545.0" layoutY="133.0" prefWidth="150.0" />
      <Text fill="WHITE" layoutX="348.0" layoutY="151.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select a theater from the list:">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <Button fx:id="toMovieSelectionsButton" layoutX="559.0" layoutY="230.0" mnemonicParsing="false" onAction="#toMovieSelections" text="To Movie Selections" />
   </children>
</AnchorPane>
